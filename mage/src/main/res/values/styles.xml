<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/md_blue_600</item>
        <item name="colorPrimaryDark">@color/md_blue_800</item>
        <item name="colorAccent">@color/md_blue_600</item>

        <item name="android:windowBackground">@color/background</item>

        <item name="preferenceTheme">@style/PreferenceThemeOverlay.v14.Material</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.NoActionBar.White">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorAccent">@color/md_white_1000</item>
    </style>

    <style name="AppTheme.Toolbar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <item name="android:actionOverflowButtonStyle">@style/AppTheme.Toolbar.Overflow</item>
    </style>

    <style name="AppTheme.Toolbar.Overflow" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:tint">@color/md_white_1000</item>
    </style>

    <style name="AppTheme.PrimaryAccent">
    </style>

    <style name="AppTheme.White">
        <item name="colorControlNormal">#8AFFFFFF</item>
        <item name="colorControlActivated">@color/md_white_1000</item>
        <item name="editTextStyle">@style/AppTheme.EditTextStyle.White</item>
    </style>

    <style name="AppTheme.TextInputLayout" parent="Widget.Design.TextInputLayout">
        <item name="colorControlNormal">?attr/colorPrimary</item>
        <item name="colorControlActivated">?attr/colorPrimary</item>
        <item name="colorControlHighlight">?attr/colorPrimary</item>
        <item name="passwordToggleTint">?attr/colorPrimary</item>
    </style>

    <style name="AppTheme.EditTextStyle.White" parent="@style/Widget.AppCompat.EditText">
        <item name="android:textColor">@color/md_white_1000</item>
    </style>

    <style name="AppTheme.ErrorTextAppearance" parent="@style/TextAppearance.AppCompat">
        <item name="android:textColor">#A8F44336</item>
    </style>

    <style name="AppTheme.Radio">
        <item name="colorControlActivated">?attr/colorPrimary</item>
    </style>

    <style name="AppTheme.SearchView" parent="Widget.AppCompat.Light.SearchView">
        <item name="searchHintIcon">@null</item>
    </style>

    <style name="AppTheme.Radio.Dark" parent="Base.ThemeOverlay.AppCompat.Dark">
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="colorControlNormal">?attr/colorPrimary</item>
        <item name="colorControlActivated">?attr/colorPrimary</item>
    </style>

    <style name="AppTheme.AlternateFAB" parent="Widget.AppCompat.Button">
        <item name="android:tint">@color/md_blue_600</item>
        <item name="colorAccent">@color/md_white_1000</item>

        <item name="colorButtonNormal">@color/md_blue_600</item>
    </style>

    <style name="AppTheme.PrimaryFAB" parent="Widget.AppCompat.Button">
        <item name="android:tint">@color/md_white_1000</item>
        <item name="colorAccent">@color/md_orange_A200</item>
    </style>

    <style name="AppTheme.PrimaryButton">
        <item name="colorButtonNormal">?attr/colorPrimary</item>
        <item name="android:textColor">@color/md_white_1000</item>
    </style>

    <style name="AppTheme.PrimaryDarkButton">
        <item name="colorButtonNormal">?attr/colorPrimaryDark</item>
        <item name="android:textColor">@color/md_white_1000</item>
    </style>

    <style name="ListSubheader">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:alpha">.54</item>
    </style>

    <style name="ListSubheaderPrimary">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="ListTextPrimary">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:alpha">.87</item>
    </style>

    <style name="ListTextSecondaryDark">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:alpha">.87</item>
    </style>

    <style name="ListTextSecondary">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:alpha">.54</item>
    </style>

    <style name="ObservationPropertyLabel">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:alpha">.87</item>
    </style>

    <style name="ObservationPropertyValue">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#8A000000</item>
    </style>

    <style name="ObservationImportant">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#D50000</item>
        <item name="android:alpha">.54</item>
    </style>

    <style name="EditTextView" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#59000000</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="ViewTextView" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#999999</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="SecondaryTextView" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#BEBEBE</item>
    </style>

    <style name="horizontal_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#333333</item>
    </style>

    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:textColorPrimary">#89000000</item>
    </style>

    <style name="AppTheme.PrimaryDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">?attr/colorPrimaryDark</item>
    </style>

    <style name="customCheckBoxStyle" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textColor">@android:color/white</item>
    </style>

</resources>